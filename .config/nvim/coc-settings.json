{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnType": true,
  "languageserver": {},
  "suggest.numberSelect": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.buffer.showHiddenBuffers": true,
  "explorer.position": "left",
  "explorer.width": 40,
  "explorer.file.showHiddenFiles": true,
  "explorer.file.column.git.showIgnored": true,
  "json.enable": true,
  "json.format.enable": true,
  "javascript.format.enabled": true,
  "typescript.format.enabled": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "highlight.document.enable": true,
  "highlight.colors.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
