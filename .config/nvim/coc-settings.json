{
	"languageserver": {
	},
	"coc.preferences.colorSupport": true,
	"coc.preferences.formatOnType": true,
	"list.source.files.args": ["--hidden", "--files"],
	"list.source.gfiles.defaultOptions": [],
	"list.source.grep.command": "rg",
	"suggest.enablePreselect": true,
	"suggest.enablePreview": true,
	"suggest.floatEnable": true,
	"suggest.detailField": "preview",
	"suggest.autoTrigger": "always",
	"suggest.numberSelect": false,
	"suggest.noselect": false,
	"suggest.keepCompleteopt": false,
	"session.saveOnVimLeave": false,
	"session.restartOnSessionLoad": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.errorSign": "",
	"diagnostic.warningSign": "",
	"diagnostic.infoSign": "",
	"diagnostic.hintSign": "ﯦ",
	"codeLens.enable": true,
	"explorer.position": "floating",
	"explorer.floating.position": "center",
	"explorer.floating.border.enable": true,
	"explorer.quitOnOpen": true,
	"explorer.openAction.strategy": "sourceWindow",
	"explorer.icon.hidden": "‥",
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.enableVimDevicons": false,
	"explorer.buffer.showHiddenBuffers": true,
	"explorer.file.showHiddenFiles": true,
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterChange",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"json.enable": true,
	"json.format.enable": true,
	"javascript.format.enabled": true,
	"typescript.format.enabled": true,
	"deno.enable": false,
	"tsserver.enable": false,
	"snippets.extends": {
		"cpp": ["c"],
		"javascriptreact": ["javascript"],
		"typescript": ["javascript"]
	},
	"highlight.document.enable": true,
	"highlight.colors.enable": true,
	"eslint.packageManager": "npm",
	"eslint.autoFixOnSave": true
}

